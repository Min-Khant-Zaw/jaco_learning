<launch>

	<!-- PID gains -->
	<arg name="p_gain" default="50"/>
	<arg name="i_gain" default="0"/>
	<arg name="d_gain" default="20"/>

	<!-- Participant ID -->
	<arg name="ID" default="1"/>

	<!-- Experimental task (0,1,2,3) -->
	<arg name="task" default="0"/> 

	<!-- Method type ('A'- impedance, 'B'- learning, 'C'- demonstration) -->
	<arg name="methodType" default="A"/>

	<!-- Demo mode ('T' - demo with optimal weights, 'F' - default weights) -->
	<arg name="demo" default="F"/>

	<!-- velocity-based with trajopt planning PID -->
	<node name="pid_trajopt" pkg="iact_control" type="pid_trajopt.py" args="$(arg p_gain) $(arg i_gain) $(arg d_gain) $(arg ID) $(arg task) $(arg methodType) $(arg demo)" output="screen"/>

</launch>
